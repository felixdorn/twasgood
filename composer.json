{
    "name": "twasgood/twasgood",
    "license": "proprietary",
    "require": {
        "php": "^8.2",
        "based/momentum-modal": "^0.2",
        "doctrine/dbal": "^3.5",
        "felixdorn/laravel-make-pivot-table": "^0.1.1",
        "felixdorn/laravel-navigation": "dev-main",
        "guzzlehttp/guzzle": "^7.2",
        "http-interop/http-factory-guzzle": "^1.2",
        "inertiajs/inertia-laravel": "^0.6.8",
        "intervention/image": "^2.7",
        "laravel/framework": "^10",
        "laravel/octane": "^1.3",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^10.2",
        "laravel/tinker": "^2.7",
        "meilisearch/meilisearch-php": "^1.2",
        "sentry/sentry-laravel": "^3.0",
        "tightenco/ziggy": "^1.0",
        "ueberdosis/tiptap-php": "^1.1",
        "ext-gd": "*",
        "ext-exif": "*",
        "laravel-lang/common": "^3.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.19",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.10",
        "nunomaduro/larastan": "^2.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ignition": "^2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {}
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "lint": "pint",
        "test:types": "phpstan analyse --memory-limit=-1",
        "test:unit": "pest",
        "test": [
            "pint --test",
            "@test:types",
            "@test:unit"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "prefer-stable": true
}
